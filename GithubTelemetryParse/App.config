<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <appSettings>
    <add key="GitHubAccount" value="" /> <!--this is your github account-->
    <add key="GitHubPersonalToken" value="" /> <!--this is your github personal access token. https://github.com/blog/1509-personal-api-tokens -->
    <add key="SQLConnectionString" value="" /> <!--this is sql ado.net connection string-->
    <add key="ReposPath" value="" /> <!--This is your github repos path ex: Microsoft/GitHubTelemetryParsor -->
  </appSettings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
      <dependentAssembly>
        <assemblyIdentity name="Newtonsoft.Json" publicKeyToken="30ad4fe6b2a6aeed" culture="neutral" />
        <bindingRedirect oldVersion="0.0.0.0-9.0.0.0" newVersion="9.0.0.0" />
      </dependentAssembly>
    </assemblyBinding>
  </runtime>
</configuration>